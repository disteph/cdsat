(set-logic FO)
(declare-sort Term 0)
(declare-fun f (Term) Bool)
(declare-fun g (Term) Bool)
(declare-fun h (Term) Bool)
(declare-fun i (Term) Bool)
(set-info :status unsat)
(assert (forall ((x Term)) (=> (or (f x) (g x)) (not (h x)))))
(assert (forall ((x Term)) (=> (=> (g x) (not (i x))) (and (f x) (h x)))))
(assert (not (forall ((x Term)) (i x))))
(check-sat)
(exit)
